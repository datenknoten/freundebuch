{
  "name": "@freundebuch/backend",
  "version": "2.46.5",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc --project tsconfig.build.json",
    "start": "node --env-file=../../.env dist/index.js",
    "test": "vitest run",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "migrate": "tsx --env-file=../../.env ../../node_modules/node-pg-migrate/bin/node-pg-migrate.js --decamelize --migrations-dir ../../database/migrations up",
    "migrate:down": "tsx --env-file=../../.env ../../node_modules/node-pg-migrate/bin/node-pg-migrate.js --decamelize --migrations-dir ../../database/migrations down",
    "migrate:create": "tsx --env-file=../../.env ../../node_modules/node-pg-migrate/bin/node-pg-migrate.js --decamelize --migrations-dir ../../database/migrations create",
    "pgtyped": "pgtyped -c pgtyped.config.json",
    "pgtyped:watch": "pgtyped -c pgtyped.config.json -w",
    "seed": "tsx --env-file=../../.env src/scripts/seed.ts"
  },
  "dependencies": {
    "@freundebuch/shared": "workspace:*",
    "@hono/node-server": "1.19.7",
    "@pgtyped/runtime": "2.4.2",
    "@sentry/node": "^10.32.1",
    "arktype": "2.1.29",
    "bcrypt": "6.0.0",
    "hono": "4.11.1",
    "jsonwebtoken": "9.0.3",
    "lru-cache": "11.2.4",
    "node-cron": "4.2.1",
    "pg": "8.16.3",
    "pino": "10.1.0",
    "pino-http": "11.0.0",
    "pino-pretty": "13.1.3",
    "rate-limiter-flexible": "9.0.1",
    "sharp": "0.34.5"
  },
  "devDependencies": {
    "@pgtyped/cli": "2.4.3",
    "@testcontainers/postgresql": "11.10.0",
    "@types/bcrypt": "6.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "25.0.3",
    "@types/node-cron": "3.0.11",
    "@types/pg": "8.16.0",
    "@types/sharp": "0.32.0",
    "@types/supertest": "6.0.3",
    "supertest": "7.1.4",
    "testcontainers": "11.10.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  }
}

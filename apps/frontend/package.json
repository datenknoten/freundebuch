    "autoprefixer": "10.4.20",
    "jsdom": "27.0.0",
    "postcss": "8.4.49",
    "svelte": "5.2.10",
    "svelte-check": "4.1.3",
    "tailwindcss": "3.4.17",
    "tslib": "2.8.1",

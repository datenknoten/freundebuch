{
  "name": "freundebuch/sabredav",
  "description": "CardDAV/CalDAV server for Freundebuch",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "sabre/dav": "^4.6",
    "ext-pdo": "*",
    "ext-pdo_pgsql": "*",
    "sentry/sdk": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "testcontainers/testcontainers": "^0.2"
  },
  "autoload": {
    "psr-4": {
      "Freundebuch\\DAV\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Freundebuch\\DAV\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "optimize-autoloader": true
  }
}
